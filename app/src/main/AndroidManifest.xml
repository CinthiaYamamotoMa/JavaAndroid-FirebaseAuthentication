<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.google.codelabs.mdc.java.firebaseAuth">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@android:drawable/btn_star"
        android:label="@string/shr_app_name"
        android:roundIcon="@android:drawable/btn_star"
        android:supportsRtl="true"
        android:theme="@style/Theme.Shrine">
        <activity android:name="org.fatec.scs.mdc.java.firebaseAuth.SignUpActivity"/>
        <activity android:name="org.fatec.scs.mdc.java.firebaseAuth.ProfileActivity"/>
        <activity android:name="org.fatec.scs.mdc.java.firebaseAuth.MainActivity"
            android:label="Login">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

    </application>
</manifest>
